{
  "name": "better-auth-supabase-plugin",
  "version": "0.0.1",
  "description": "Supabase JWT authentication plugin for Better Auth",
  "keywords": [
    "better-auth",
    "supabase",
    "jwt",
    "authentication"
  ],
  "homepage": "https://github.com/avjco/betterauth-supabase-plugin#readme",
  "bugs": {
    "url": "https://github.com/avjco/betterauth-supabase-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avjco/betterauth-supabase-plugin.git"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client-plugin.d.ts",
      "import": "./dist/client-plugin.js",
      "default": "./dist/client-plugin.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./index.ts ./client-plugin.ts --outdir ./dist --target node --minify --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "prepublishOnly": "bun run build && bun run build:types",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "better-auth": "^1.2.3",
    "jose": "^6.0.10"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
